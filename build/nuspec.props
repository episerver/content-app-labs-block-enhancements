<Project>
    <PropertyGroup>
        <IsPackable>true</IsPackable>
        <NuspecBasePath>$(ProjectDir)..\..\</NuspecBasePath>
        <PackageVersion Condition=" '$(PackageVersion)' == '' ">$(VersionPrefix)$(VersionSuffix)</PackageVersion>
        <NuspecProperties>Configuration=$(Configuration);PackageVersion=$(PackageVersion);CmsCoreVersion=$(CmsCoreVersion);CmsUIVersion=$(CmsUIVersion);CmsUINextMajorVersion=$(CmsUINextMajorVersion);</NuspecProperties>
        <PackageOutputPath>$(SolutionDir)artifacts\packages\</PackageOutputPath>
        <NoWarn>NU5100;NU5125</NoWarn>
    </PropertyGroup>
</Project>
