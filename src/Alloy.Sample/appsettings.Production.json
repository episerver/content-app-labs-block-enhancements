{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "EPiServer": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "EPiServerDB": "Data Source={DbServer};Database={DbDatabase};User Id={DbUserName};Password={DbPassword};MultipleActiveResultSets=True"
    }
}